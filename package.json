{
  "name": "test-pwa",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --ci --coverage",
    "test:bundlesize": "bundlewatch",
    "serve": "serve -s build",
    "gc": "gitmoji -c",
    "gca": "git add . && yarn gc",
    "gcap": "yarn gca && git push",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress:open": "cypress open",
    "env": "node ./scripts/set_env",
    "generate-antd-themes": "npx gulp less"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.1",
    "@fortawesome/pro-regular-svg-icons": "^5.15.1",
    "@fortawesome/pro-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "antd": "^4.10.0",
    "jotai": "^0.12.3",
    "react": "^17.0.1",
    "react-css-theme-switcher": "^0.2.2",
    "react-dom": "^17.0.1",
    "react-query": "^3.5.11",
    "web-vitals": "^1.0.1",
    "workbox-background-sync": "^6.0.2",
    "workbox-broadcast-update": "^6.0.2",
    "workbox-cacheable-response": "^6.0.2",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-google-analytics": "^6.0.2",
    "workbox-navigation-preload": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-range-requests": "^6.0.2",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2",
    "workbox-streams": "^6.0.2"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx": "^7.10.4",
    "@changesets/cli": "^2.12.0",
    "@jest/types": "^26.6.2",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "9.0.0",
    "babel-loader": "8.1.0",
    "bundlesize": "^0.18.1",
    "bundlewatch": "^0.3.1",
    "cypress": "^6.2.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "8.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "prettier": "2.2.1",
    "react-app-rewire-alias": "^0.1.9",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "webpack": "4.44.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "json"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./build/static/js/*.js",
        "maxSize": "250kB"
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
